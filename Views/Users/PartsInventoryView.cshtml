<script>

     SubmitEvent();

     function SubmitEvent(e) { getView() };

     async function getView() {
         try {
             const response = await $.ajax({
                 url: '/not-implemented'
             });

             $('.comming-soon').html(response);
         } catch (err) {
             console.error(err);
             $('.comming-soon div').html('<div class="info-container"></div>');
             $('.info-container')
                 .append(`<h1>Feature not yet implemented</h1>`)
                 .append('<i class="bi bi-cone-striped fs-3"></i>')
                 .append(`<br/><i class="fa fa-exclamation-triangle" aria-hidden="true"></i>`)
                 .append('This feature is currently underconstruction')
                 .append(`<i class="fa fa-exclamation-triangle" aria-hidden="true"></i>`)
                 .append('<br/><span class="spinner-container"></span>')
                 .addClass('border p-4 rounded shadow');

             for (let element = 0; element <= 2; element++) {
                 $('.spinner-container').append('<span class="spinner-grow" role="status"></span>');
             }
         }
     }
</script>

